{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		// "module": "ESNext",
		// "moduleResolution": "Node",
		// "target": "ESNext",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"noEmitOnError": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noEmit": false,
		"strict": true,
		"composite": true,
    "sourceRoot": ".graphclient",
		"outDir": "dist"
	},
	"include": [
		".graphclient/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
